<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">



    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#ffff6308"/>
            <corners android:radius="7dp" />
